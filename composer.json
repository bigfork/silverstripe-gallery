{
  "name": "i-lateral/silverstripe-gallery",
  "description": "A Silverstripe image gallery allowing multi image upload and automatic generation of thumbnail gallery with popups",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/i-lateral/silverstripe-gallery",
    "keywords": [
        "silverstripe",
        "gallery",
        "thumbnails",
        "lightbox",
        "modal"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@ilateral.co.uk"
        }
    ],
    "support": {
        "issues": "http://github.com/i-lateral/silverstripe-gallery/issues"
    },
    "require": {
        "silverstripe/cms": "^5",
        "bummzack/sortablefile": "^2"
    },
    "extra": {
        "expose": [
            "client/dist",
            "node_modules"
        ]
    },
    "autoload": {
        "psr-4": {
            "ilateral\\SilverStripe\\Gallery\\": "src/"
        }
    }
}
